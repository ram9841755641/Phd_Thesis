}
if (position == 'Right2') {
document.write ('<A HREF="http://oas.rsc.org/5c/pubs.rsc.org/en/journals/journalissues/ob/L41/1884480385/Right2/RSC/2010_CS_Call4papers/120x90Recognition.gif/6131685077464769634f6f4141523747?x" target=" "><IMG SRC="http://imagec16.247realmedia.com/0/RSC/2010_CS_Call4papers/120x90Recognition.gif"  ALT="Chemical Science, recognition starts here"  BORDER="0"></A>');
}
if (position == 'x01') {
