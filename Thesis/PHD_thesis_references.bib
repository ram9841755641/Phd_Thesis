	keywords = {biotechnology, Genetics and genomics, Materials, Methods, molecular biology},
	pages = {348--352},
	file = {Full Text PDF:/Users/fredricknindo/Library/Application Support/Firefox/Profiles/ajyuo9gs.default/zotero/storage/2TIBRU2D/Rothberg et al. - 2011 - An integrated semiconductor device enabling non-op.pdf:application/pdf;Snapshot:/Users/fredricknindo/Library/Application Support/Firefox/Profiles/ajyuo9gs.default/zotero/storage/UAXVQE23/nature10242.html:text/html}
