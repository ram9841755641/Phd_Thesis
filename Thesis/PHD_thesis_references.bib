	note = {{PMID:} 10846592},
	keywords = {Anti-{HIV} Agents, Clinical Trials as Topic, Cohort Studies, Data Interpretation, Statistical, Drug Resistance, Microbial, Drug Therapy, Combination, Genotype, {HIV} Infections, {HIV-1}, Humans, Microbial Sensitivity Tests, Phenotype, Prospective Studies, Retrospective Studies, Reverse Transcriptase Inhibitors, {RNA}, Viral, Treatment Outcome},
