	pages = {e4012},
	file = {PLoS Full Text PDF:/Users/fredricknindo/Library/Application Support/Firefox/Profiles/ajyuo9gs.default/zotero/storage/RN2KPFWG/Shen et al. - 2008 - Comparing Platforms for C. elegans Mutant Identifi.pdf:application/pdf;PLoS Snapshot:/Users/fredricknindo/Library/Application Support/Firefox/Profiles/ajyuo9gs.default/zotero/storage/IRM525UV/infodoi10.1371journal.pone.html:text/html}
