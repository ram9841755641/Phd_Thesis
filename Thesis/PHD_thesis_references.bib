@article{turner_tipranavir_1998,
	title = {Tipranavir ({PNU-140690):}  A Potent, Orally Bioavailable Nonpeptidic {HIV} Protease Inhibitor of the 5,6-Dihydro-4-hydroxy-2-pyrone Sulfonamide Class∇},
	volume = {41},
