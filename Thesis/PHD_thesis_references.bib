	file = {ScienceDirect Snapshot:/Users/fredricknindo/Library/Application Support/Firefox/Profiles/ajyuo9gs.default/zotero/storage/TPZP86WU/016635429190045S.html:text/html}
},

@article{kempf_abt-538_1995,
	title = {{ABT-538} is a potent inhibitor of human immunodeficiency virus protease and has high oral bioavailability in humans.},
